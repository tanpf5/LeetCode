
public class Solution {
    public static int titleToNumber(String s) {
    	int ans = 0;
        while (s != "") {
        	char c = s.charAt(0);
        	ans = ans * 26 + (int)c - (int)'A';
        	s = s.substring(1);
        }
        return ans;
    }
    
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(titleToNumber("AA"));
	}

}
