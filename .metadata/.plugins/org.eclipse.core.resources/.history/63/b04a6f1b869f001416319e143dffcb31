
public class Solution {

    public static int compareVersion(String version1, String version2) {
        while (version1 != "" && version2 != "") {
        	int i = version1.indexOf('.') == -1 ? version1.length() : version1.indexOf('.');
        	int v1 = Integer.parseInt(version1.substring(0, i - 1));
        	int j = version2.indexOf('.') == -1 ? version2.length() : version2.indexOf('.');
        	int v2 = Integer.parseInt(version2.substring(0, j - 1));
        	if (v1 == v2) {
        		version1 = version1.substring(i + 1);
        		version2 = version2.substring(j + 1);
        	}
        	if (v1 > v2) return 1;
        	if (v1 < v2) return -1;
        }
        if (version1 != "") return 1;
        if (version2 != "") return -1;
        return 0;
    }
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(compareVersion("123.234", "123.43"));
	}

}
